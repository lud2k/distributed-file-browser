{
  "name": "distributed-file-browser",
  "version": "1.0.1",
  "description": "A simple web UI to browse files on one or many servers",
  "main": "index.js",
  "scripts": {
    "build:backend": "webpack --config=webpack-backend-config.js",
    "build:frontend": "webpack --config=webpack-frontend-config.js",
    "dev:backend": "DEV=true MODE=ips HOSTS=127.0.0.1:3000 HOST=127.0.0.1:3000 DIRECTORY=\"/var/log\" webpack --watch --config=webpack-backend-config.js",
    "dev:backend-1": "DEV=true MODE=ips SERVER_PORT=3001 HOSTS=127.0.0.1:3001,127.0.0.1:3002 HOST=127.0.0.1:3001 node dist/backend.js",
    "dev:backend-2": "DEV=true MODE=ips SERVER_PORT=3002 HOSTS=127.0.0.1:3001,127.0.0.1:3002 HOST=127.0.0.1:3002 node dist/backend.js",
    "dev:frontend": "DEV=true webpack-dev-server --config=webpack-frontend-config.js",
    "start": "node dist/backend.js"
  },
  "author": "Ludovic Cabre",
  "license": "ISC",
  "dependencies": {
    "@kubernetes/client-node": "^0.12.0",
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.155",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/tail": "^2.0.0",
    "@types/tiny-async-pool": "^1.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "moment": "^2.26.0",
    "node-fetch": "^2.6.0",
    "query-string": "^6.13.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "request": "^2.88.2",
    "serve-static": "^1.14.1",
    "tail": "^2.0.3",
    "tiny-async-pool": "^1.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2"
  }
}
